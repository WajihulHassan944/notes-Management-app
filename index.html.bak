<style>
body{
	min-height:100vh;
	width:100%;
	 background:url(hero-bg.jpg) center center no-repeat;
    background-size: cover;

}
.contactbtn{
	position:absolute;
	top:2.9%;
	right:5%;
	padding:10px 30px;
	border-radius:20px;
	color:#fff;
	background:#fb523c;
}
.box{
	width:250px;
	height:200px;
	margin-right:30px;
	border-radius:10px;
	position:relative;
	padding:15px;
	background:#eee;
	border:1px solid #05a5d1;
	box-shadow:0 10px 20px rgba(0,0,0,0.2);
}
.box:nth-child(3),
.box:last-child {
    margin-right:0px;
}
.boxwrap{
	display:flex;
	flex-direction:column;
}
.boxwrap .box-row{
	display:flex;
	margin-top:30px;
}
.box h1{
	font-size:20px;
	color:#091e3e;
}
.box p{
	color:#8b8d90;
	margin-top:20px;
	margin-bottom:40px;
}
.box .icons{
	width:100%;
	display:flex;
	justify-content:center;
	align-items:center;
	gap:10px;
}
.box .icons a{
	padding:6px 20px;
	background:#091e3e;
	color:#fff;
	border-radius:10px;
	text-decoration:none;
}

</style>
